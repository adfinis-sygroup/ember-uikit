  "ember": {
    "edition": "octane"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://adfinis-sygroup.github.io/ember-uikit"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "extends": "@adfinis-sygroup/semantic-release-config"
  }
}
