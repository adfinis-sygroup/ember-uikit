{
  "name": "ember-uikit",
  "version": "2.2.2",
  "description": "The ember implementation of UIkit",
  "keywords": [
    "ember-addon",
    "uikit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adfinis-sygroup/ember-uikit"
  },
  "license": "MIT",
  "author": "Adfinis SyGroup AG <info@adfinis-sygroup.ch>",
  "directories": {
    "test": "tests"
  },
  "contributors": [
    {
      "name": "Jonas Metzener",
      "email": "jonas.metzener@adfinis-sygroup.ch"
    }
